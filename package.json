    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "init-project": "npm install -g bower && npm install -g gulp && npm run-script develop",
        "develop-project": "npm install && bower install && npm run-script install",
        "install-project": "composer install && php vendor/robmorgan/phinx/bin/phinx migrate",
        "upgrade-project": "composer update && php vendor/robmorgan/phinx/bin/phinx migrate",
        "update-project": "npm update && composer update && npm run-script assets",
        "assets-project": "bower update && npm run-script build",
        "build-project": "gulp",
        "reset-project": "php vendor/robmorgan/phinx/bin/phinx rollback -t 0 && php vendor/robmorgan/phinx/bin/phinx migrate"
    }
