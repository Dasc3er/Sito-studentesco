    "main": "gulpfile.js",
    "dependencies": {},
    "devDependencies": {
        "bower-main": "^0.2.14",
        "cwd": "^0.10.0",
        "del": "^2.2.0",
        "gulp": "^3.9.1",
        "gulp-concat": "^2.6.0",
        "gulp-concat-css": "^2.3.0",
        "gulp-flatten": "^0.2.0",
        "gulp-json-minify": "^1.0.5",
        "gulp-rename": "^1.2.2",
