{
  "name": "slim-skeleton",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "^3.3.6",
    "tinymce": "^4.4.0",
    "jquery.complexify": "danpalmer/jquery.complexify.js#^0.5.1",
    "dropzone": "^4.3.0",
    "font-awesome": "^4.6.3",
    "php-debugbar": "maximebf/php-debugbar#^1.13.0",
    "cookieconsent": "^3.0.3"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.min.js",
        "dist/css/bootstrap.min.css"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.min.css",
        "fonts/*"
      ]
    }
  }
}
