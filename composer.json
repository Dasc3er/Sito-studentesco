{
    "name": "dasc3er/slim-skeleton",
    "description": "",
    "keywords": ["slim", "skleton"],
    "type": "project",
    "license": "MIT",
    "require": {
        "Respect/Validation": "^1.1",
        "dasc3er/slim-whoops": "^0.1.0",
        "ezyang/htmlpurifier": "^4.8",
        "illuminate/database": "5.2.*",
        "illuminate/pagination": "5.2.*",
        "maximebf/debugbar": "^1.13",
        "monolog/monolog": "^1.21",
        "phpmailer/phpmailer": "^5.2",
        "robmorgan/phinx": "^0.6.5",
        "slim/csrf": "dev-master as 0.8.x-dev",
        "slim/flash": "^0.1.0",
        "slim/slim": "^3.4",
        "slim/twig-view": "^2.1",
        "symfony/asset": "^3.1",
        "symfony/translation": "^3.1",
        "symfony/twig-bridge": "^3.1",
        "symfony/yaml": "^3.1",
        "twig/extensions": "^1.3"
    },
    "require-dev": {
        "codeception/codeception": "^2.2",
        "friendsofphp/php-cs-fixer": "^1.12",
        "fzaninotto/faker": "^1.6"
    },
    "repositories": [{
        "type": "vcs",
        "url": "git://github.com/alexweissman/Slim-Csrf"
    }],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "app/libs/"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "prefer-stable": true
    }
}
